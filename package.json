{
  "name": "deviltea-site-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "npm run pre-build && vue-cli-service serve",
    "build": "npm run pre-build && vue-cli-service build",
    "build:analyze": "npm run build -- --analyze",
    "pre-build": "npm run generate:article-api && npm run generate:icons-import",
    "generate:article-api": "ts-node -r tsconfig-paths/register --project ./scripts/tsconfig.json ./scripts/generateArticleAPI.ts",
    "generate:icons-import": "ts-node -r tsconfig-paths/register --project ./scripts/tsconfig.json ./scripts/generateIconsImportion.ts",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "@mdi/js": "^4.9.95",
    "axios": "^0.19.2",
    "cheerio": "^1.0.0-rc.3",
    "core-js": "^3.6.4",
    "lodash-es": "^4.17.15",
    "markdown-it": "^10.0.0",
    "markdown-it-abbr": "^1.0.4",
    "markdown-it-anchor": "^5.2.7",
    "markdown-it-container": "^2.0.0",
    "markdown-it-deflist": "^2.0.3",
    "markdown-it-emoji": "^1.4.0",
    "markdown-it-external-links": "0.0.6",
    "markdown-it-footnote": "^3.0.2",
    "markdown-it-imsize": "^2.0.1",
    "markdown-it-ins": "^3.0.0",
    "markdown-it-mark": "^3.0.0",
    "markdown-it-sub": "^1.0.0",
    "markdown-it-sup": "^1.0.0",
    "markdown-it-toc-done-right": "^4.1.0",
    "path-browserify": "^1.0.1",
    "prismjs": "^1.19.0",
    "vue": "^2.6.11",
    "vue-class-component": "^7.2.2",
    "vue-codemirror": "^4.0.6",
    "vue-property-decorator": "^8.3.0",
    "vue-router": "^3.1.5",
    "vue-scrollto": "^2.18.1",
    "vuex": "^3.1.2",
    "vuex-module-decorators": "^0.16.1"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.18",
    "@types/lodash-es": "^4.17.3",
    "@types/markdown-it": "0.0.9",
    "@types/node": "^13.13.2",
    "@types/prismjs": "^1.16.0",
    "@typescript-eslint/eslint-plugin": "^2.18.0",
    "@typescript-eslint/parser": "^2.18.0",
    "@vue/cli-plugin-babel": "~4.2.0",
    "@vue/cli-plugin-eslint": "~4.2.0",
    "@vue/cli-plugin-router": "~4.2.0",
    "@vue/cli-plugin-typescript": "^4.3.1",
    "@vue/cli-plugin-vuex": "~4.2.0",
    "@vue/cli-service": "~4.2.0",
    "@vue/eslint-config-standard": "^5.1.0",
    "@vue/eslint-config-typescript": "^5.0.1",
    "babel-plugin-prismjs": "^2.0.1",
    "eslint": "^6.7.2",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^6.1.2",
    "lint-staged": "^9.5.0",
    "node-sass": "^4.14.1",
    "prerender-spa-plugin": "^3.4.0",
    "sass-loader": "^8.0.2",
    "ts-node": "^8.9.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "~3.7.5",
    "vue-cli-plugin-prerender-spa": "~1.1.6",
    "vue-template-compiler": "^2.6.11",
    "webpack-bundle-analyzer": "^3.6.1"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,vue,ts,tsx}": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
